{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": false,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },

    "types": [
      "@nuxt/types",
      "@types/node",
      "@nuxtjs/auth",
      "@types/bootstrap",
      "@nuxt/kit", 
      "@nuxtjs/axios",
    ]
  },
  "exclude": ["node_modules"]
}
